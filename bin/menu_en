         ofp
         return
         main_menu
         return
         break 
         elif [ "$REPLY" == "$i" ]; then
         clear
        main_menu
        return
        break
        else
        clear
        echo
            echo "      Enter the numbers corresponding to the menu."
         ofp_extract
         return
         break
         fi
         break;;
   *) clear
        echo
            echo "      Enter the numbers corresponding to the menu."
         ofp_extract
         return
         break ;;
