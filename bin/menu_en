        set -- $outdir
         return
         break
         fi
         break;;
   *) clear
        #echo
            #echo "      Enter the numbers corresponding to the menu."
         outdir="/data/local/UnpackerSystem"
         set -- $outdir
         return
         break ;;
  esac
 done
 else
