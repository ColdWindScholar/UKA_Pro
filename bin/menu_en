reb()
{
echo
 echo -ne "        Reboot after 5 sec      \r"
sleep 1
echo -ne "         ..... 5 .....                \r"
sleep 1
echo -ne "          .... 4 ....                 \r"
sleep 1
echo -ne "           ... 3 ...                  \r"
sleep 1
echo -ne "            .. 2 ..                   \r"
sleep 1
echo -ne "             . 1 .                    \r"
sleep 1
echo -ne "                                      \r"
}

             del_unpack()
{
echo
echo -en "\E[31;47;1m"
echo "     Remove \"Unpacker Kitchen for Android\" from your phone?     "
echo -en "\E[37;0m"
